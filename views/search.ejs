<%- include("partials/header"); -%>

<div class="container">
  <h3 class="text-center">SEARCH RESULTS:</h3>
</div>

<% posts.forEach(function(post){ %>
<div class="posts container">
  <a href="/posts/<%=post._id%>">
    <h1><%=post.title%></h1>
    <p>
      <%-post.content.substring(0, 400) + " ..."%>
    </p>
  </a>
</div>

<% }) %> <%- include("partials/footer"); -%>
